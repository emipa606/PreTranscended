<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelBase">
    <defName>Apparello_PTArmorDROP</defName>
    <label>Transcended Armor</label>
    <description>Pre transcended technology. Fitted with selfdestructive systemsFitted with selfdestructive systems (lucky for you it seems to be defective! and so can in fact be removed from the wearer without breaking.)</description>
    <graphicData>
      <texPath>PreAcs/PreAcs</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>Sellable</tradeability>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>350</MaxHitPoints>
      <MarketValue>1000</MarketValue>
      <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.7</ArmorRating_Sharp>
      <ArmorRating_Heat>0.3</ArmorRating_Heat>
      <Insulation_Cold>25</Insulation_Cold>
      <Insulation_Heat>10</Insulation_Heat>
      <Mass>3</Mass>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>1</MoveSpeed>
      <WorkSpeedGlobal>-0.25</WorkSpeedGlobal>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>PreAcs/PreAcs</wornGraphicPath>
      <layers>
        <li>Middle</li>
        <li>Shell</li>
      </layers>
      <tags>
        <li>PreTranny</li>
      </tags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>20</weight>
          <only>RGBA(1,1,1,1)</only>
        </li>
        <li>
          <weight>20</weight>
          <only>RGBA(1,0.9,0.8,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
</Defs>