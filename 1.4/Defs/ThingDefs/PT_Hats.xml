<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelBase">
    <defName>Apparello_PTAerodynamic</defName>
    <label>Aerodynamic Visor</label>
    <description>Pre transcended technology. Fitted with selfdestructive systems, cannot be removed from the wearer without breaking.</description>
    <graphicData>
      <texPath>PreAcs/AcsHelm</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>None</tradeability>
    <destroyOnDrop>True</destroyOnDrop>
    <statBases>
      <MarketValue>1000</MarketValue>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.53</ArmorRating_Sharp>
      <Insulation_Heat>10</Insulation_Heat>
      <Insulation_Cold>10</Insulation_Cold>
      <Mass>1</Mass>
      <EquipDelay>0.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.15</SocialImpact>
      <MoveSpeed>-0.02</MoveSpeed>
      <PsychicSensitivity>-0.05</PsychicSensitivity>
      <MentalBreakThreshold>-0.01</MentalBreakThreshold>
      <GlobalLearningFactor>0.02</GlobalLearningFactor>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>PreAcs/AcsHelm</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>PreTranny</li>
      </tags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>20</weight>
          <only>RGBA(1,1,1,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
