      </options>
    </colorGenerator>
  </ThingDef>
  <!--Commissar-->
  <ThingDef ParentName="HatBase">
    <defName>Apparello_PTCommissar</defName>
    <label>Peaked cap</label>
    <description>Often seen on military officers.</description>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <texPath>PreAcs/DisCommissar</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>Sellable</tradeability>
    <statBases>
      <MarketValue>250</MarketValue>
      <ArmorRating_Blunt>0.08</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.08</ArmorRating_Sharp>
      <Insulation_Cold>1</Insulation_Cold>
      <Insulation_Heat>-1</Insulation_Heat>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1</DeteriorationRate>
      <SellPriceFactor>0.5</SellPriceFactor>
      <Mass>1</Mass>
      <EquipDelay>0.5</EquipDelay>
    </statBases>
    <costList>
      <Cloth>50</Cloth>
    </costList>
    <equippedStatOffsets>
      <MentalBreakThreshold>-0.10</MentalBreakThreshold>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>PreAcs/Commissar</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>PTHattegat</li>
      </tags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>4</weight>
          <only>RGBA(1,1,1,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
